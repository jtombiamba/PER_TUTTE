#ifndef LIST_H
#define LIST_H

struct pair_s
{
  void *elt;
  struct pair_s *next;
};

typedef struct pair_s pair;

typedef struct
{
  pair *first;
  pair *last;
}list;

list *create_list(void);
int empty_list(list *l);
int add_to_list(list *l, void *elt);
int add_to_end_list(list *l, void *elt);
void *remove_from_list(list *l);

#endif	/* LIST_H */
